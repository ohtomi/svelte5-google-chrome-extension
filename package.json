    "check": "svelte-check --tsconfig ./tsconfig.json",
    "postinstall": "wxt prepare"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.2",
    "@tsconfig/svelte": "^5.0.4",
    "@wxt-dev/module-svelte": "^1.0.1",
    "playwright": "^1.48.2",
    "svelte": "^5.0.3",
    "svelte-check": "^4.0.5",
    "tslib": "^2.8.0",
    "typescript": "^5.6.2",
    "wxt": "^0.19.11"
  },
  "overrides": {
    "@sveltejs/vite-plugin-svelte": "^4.0.0"
  }
}
